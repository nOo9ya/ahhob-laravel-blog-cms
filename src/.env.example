APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

# JWT Authentication
JWT_SECRET=
JWT_TTL=60
JWT_REFRESH_TTL=20160
JWT_ALGO=HS256
JWT_BLACKLIST_ENABLED=true
JWT_BLACKLIST_GRACE_PERIOD=0

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=file
CACHE_PREFIX=

# Blog Caching Settings
BLOG_CACHE_ENABLED=true
BLOG_CACHE_PREFIX=ahhob_blog
BLOG_CACHE_USE_TAGS=true
BLOG_CACHE_WARMUP=false
BLOG_CACHE_DEBUG=false
BLOG_CACHE_LOG_HITS=false
BLOG_CACHE_LOG_MISSES=false

# Cache TTL Settings (seconds)
BLOG_CACHE_POSTS_TTL=3600
BLOG_CACHE_CATEGORIES_TTL=7200
BLOG_CACHE_TAGS_TTL=3600
BLOG_CACHE_PAGES_TTL=7200
BLOG_CACHE_COMMENTS_TTL=1800
BLOG_CACHE_STATS_TTL=1800
BLOG_CACHE_STATIC_TTL=43200
BLOG_CACHE_SEARCH_TTL=900

# Image Optimization Settings
IMAGE_OPTIMIZATION_ENABLED=true
IMAGE_MAX_WIDTH=2000
IMAGE_MAX_HEIGHT=2000
IMAGE_DEFAULT_QUALITY=90

# WebP Conversion
IMAGE_CONVERT_TO_WEBP=true
IMAGE_WEBP_QUALITY=85

# Thumbnail Generation
IMAGE_GENERATE_THUMBNAILS=true
THUMBNAIL_SIZE_WIDTH=150
THUMBNAIL_SIZE_HEIGHT=150
THUMBNAIL_QUALITY=85

SMALL_SIZE_WIDTH=300
SMALL_SIZE_HEIGHT=200
SMALL_QUALITY=90

MEDIUM_SIZE_WIDTH=600
MEDIUM_SIZE_HEIGHT=400
MEDIUM_QUALITY=90

LARGE_SIZE_WIDTH=1200
LARGE_SIZE_HEIGHT=800
LARGE_QUALITY=85

# Image Cache
IMAGE_CACHE_ENABLED=true
IMAGE_CACHE_TTL=86400

# Image Security & Cleanup
IMAGE_AUTO_CLEANUP=false
IMAGE_SCAN_MALWARE=false
IMAGE_STRIP_EXIF=false
IMAGE_WATERMARK_ENABLED=false
IMAGE_WATERMARK_TEXT=""
IMAGE_WATERMARK_OPACITY=50

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Analytics & Tracking
# Google Analytics 4
GA_ENABLED=false
GA_MEASUREMENT_ID=
GA_ANONYMIZE_IP=true
GA_COOKIE_DOMAIN=auto
GA_COOKIE_EXPIRES=63072000

# Google Tag Manager
GTM_ENABLED=false
GTM_ID=

# Google AdSense
ADSENSE_ENABLED=false
ADSENSE_CLIENT_ID=
ADSENSE_AUTO_ADS=true

# Analytics Privacy & Consent
ANALYTICS_PRIVACY_MODE=true
ANALYTICS_REQUIRE_CONSENT=true
ANALYTICS_CONSENT_COOKIE=analytics_consent
ANALYTICS_CONSENT_DURATION=365

# Analytics Development
ANALYTICS_DEBUG_MODE=false
ANALYTICS_TEST_MODE=true

# Security Configuration
# XSS Protection
XSS_PROTECTION_ENABLED=true
CSP_ENABLED=true

# CSRF Protection
CSRF_PROTECTION_ENABLED=true
CSRF_TOKEN_LIFETIME=120
CSRF_VERIFY_REFERER=true
CSRF_VERIFY_ORIGIN=true

# Password Policy
PASSWORD_POLICY_ENABLED=true
PASSWORD_MIN_LENGTH=8
PASSWORD_MAX_LENGTH=128
PASSWORD_REQUIRE_UPPERCASE=true
PASSWORD_REQUIRE_LOWERCASE=true
PASSWORD_REQUIRE_NUMBERS=true
PASSWORD_REQUIRE_SYMBOLS=true

# Password History
PASSWORD_HISTORY_ENABLED=true
PASSWORD_HISTORY_COUNT=5

# Password Expiration
PASSWORD_EXPIRATION_ENABLED=false
PASSWORD_EXPIRATION_DAYS=90
PASSWORD_EXPIRATION_WARNING_DAYS=7

# Account Lockout
PASSWORD_LOCKOUT_ENABLED=true
PASSWORD_MAX_ATTEMPTS=5
PASSWORD_LOCKOUT_DURATION=15
PASSWORD_ATTEMPTS_DECAY=60

# Password Confirmation
PASSWORD_CONFIRMATION_ENABLED=true
PASSWORD_CONFIRMATION_TIMEOUT=10800

# Session Security
SESSION_SECURITY_ENABLED=true
SESSION_REGENERATE_INTERVAL=300
SESSION_VERIFY_IP=true
SESSION_IP_CHANGE_ACTION=logout
SESSION_VERIFY_USER_AGENT=true
SESSION_CONCURRENT_LIMIT_ENABLED=false
SESSION_MAX_CONCURRENT=3
SESSION_CONCURRENT_ACTION=logout_oldest

# Input Validation
INPUT_VALIDATION_ENABLED=true
FILE_SCAN_MALWARE=false
FILE_MAX_SIZE=10485760

# Rate Limiting
RATE_LIMITING_ENABLED=true
LOGIN_MAX_ATTEMPTS=5
LOGIN_ATTEMPTS_DECAY=60
LOGIN_LOCKOUT_DURATION=60
WEB_RATE_LIMIT=1000
WEB_RATE_DECAY=1
API_RATE_LIMIT=100
API_RATE_DECAY=1

# Security Headers
SECURITY_HEADERS_ENABLED=true
HSTS_ENABLED=true
HSTS_MAX_AGE=31536000
HSTS_INCLUDE_SUBDOMAINS=true
HSTS_PRELOAD=false
X_FRAME_OPTIONS=DENY
X_CONTENT_TYPE_OPTIONS=nosniff
X_XSS_PROTECTION="1; mode=block"
REFERRER_POLICY="strict-origin-when-cross-origin"

# Security Monitoring
SECURITY_MONITORING_ENABLED=true
SECURITY_LOG_LEVEL=warning
SECURITY_NOTIFICATIONS_ENABLED=false
SECURITY_REPORTS_ENABLED=false
SECURITY_REPORT_FREQUENCY=weekly
SECURITY_REPORT_RECIPIENTS=""
